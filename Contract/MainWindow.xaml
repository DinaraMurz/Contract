<Window x:Class="Contract.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Contract"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Печать" HorizontalAlignment="Left" Margin="685,370,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Введите ФИО" VerticalAlignment="Top" Height="25" Width="150"/>
        <TextBox HorizontalAlignment="Left" Margin="197,10,0,0" TextWrapping="Wrap" Text="{Binding Path=FullName}" VerticalAlignment="Top" Height="25" Width="150"/>
        
        <TextBlock HorizontalAlignment="Left" Margin="10,40,0,0" TextWrapping="Wrap" Text="Введите адрес" VerticalAlignment="Top" Height="25" Width="150"/>
        <TextBox HorizontalAlignment="Left" Margin="197,40,0,0" TextWrapping="Wrap" Text="{Binding Path=Address}" VerticalAlignment="Top" Height="25" Width="150"/>

        
        
    </Grid>
</Window>
